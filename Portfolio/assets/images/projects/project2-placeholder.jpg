      <stop offset="0%" style="stop-color:#7c3aed" />
      <stop offset="100%" style="stop-color:#5a21a8" />
    </linearGradient>
  </defs>
  
  <rect width="350" height="250" fill="url(#proj2)"/>
  
  <!-- Task list icon -->
