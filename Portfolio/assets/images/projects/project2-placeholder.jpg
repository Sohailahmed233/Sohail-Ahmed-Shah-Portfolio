    <rect x="-50" y="-40" width="100" height="80" rx="5" fill="none" stroke="#ffffff" stroke-width="2"/>
    
    <!-- Checked items -->
    <circle cx="-35" cy="-25" r="6" fill="none" stroke="#ffffff" stroke-width="2"/>
    <path d="M -42 -25 L -32 -15 L -28 -25" fill="none" stroke="#10b981" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
    
    <line x1="-20" y1="-25" x2="35" y2="-25" stroke="#ffffff" stroke-width="2"/>
    
    <!-- Unchecked item -->
    <circle cx="-35" cy="-5" r="6" fill="none" stroke="#ffffff" stroke-width="2"/>
    <line x1="-20" y1="-5" x2="35" y2="-5" stroke="#ffffff" stroke-width="2" opacity="0.7"/>
    
    <!-- Another item -->
    <circle cx="-35" cy="15" r="6" fill="none" stroke="#ffffff" stroke-width="2"/>
    <line x1="-20" y1="15" x2="35" y2="15" stroke="#ffffff" stroke-width="2" opacity="0.7"/>
