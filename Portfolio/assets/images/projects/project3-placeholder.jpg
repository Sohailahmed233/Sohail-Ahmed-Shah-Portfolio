<svg width="350" height="250" viewBox="0 0 350 250" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="proj3" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0ea5e9" />
      <stop offset="100%" style="stop-color:#0369a1" />
    </linearGradient>
  </defs>
  
  <rect width="350" height="250" fill="url(#proj3)"/>
  
  <!-- Weather icon -->
  <g transform="translate(175, 100)">
    <!-- Sun -->
    <circle cx="-20" cy="-20" r="15" fill="#fbbf24"/>
    
    <!-- Sun rays -->
    <g stroke="#fbbf24" stroke-width="2" stroke-linecap="round">
      <line x1="-20" y1="-40" x2="-20" y2="-50"/>
      <line x1="-20" y1="0" x2="-20" y2="10"/>
      <line x1="-40" y1="-20" x2="-50" y2="-20"/>
      <line x1="0" y1="-20" x2="10" y2="-20"/>
    </g>
    
    <!-- Cloud -->
    <g fill="#ffffff">
      <ellipse cx="15" cy="10" rx="25" ry="15"/>
      <ellipse cx="0" cy="15" rx="20" ry="12"/>
      <ellipse cx="30" cy="15" rx="18" ry="12"/>
    </g>
    
    <!-- Rain drops -->
    <g stroke="#ffffff" stroke-width="2" stroke-linecap="round" opacity="0.7">
      <line x1="5" y1="30" x2="5" y2="40"/>
      <line x1="20" y1="32" x2="20" y2="42"/>
      <line x1="35" y1="30" x2="35" y2="40"/>
    </g>
  </g>
  
  <text x="175" y="200" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#ffffff" text-anchor="middle">WEATHER APP</text>
  <text x="175" y="225" font-family="Arial, sans-serif" font-size="12" fill="#ffffff" text-anchor="middle" opacity="0.8">Real-time Weather Data</text>
</svg>
